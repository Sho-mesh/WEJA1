<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.jspiders.springcorexml.beans.PassengerBean"
		name="passenger1">
		<property name="id" value="1" />
		<property name="name" value="Ajinkya" />
		<property name="age" value="25" />
		<property name="gender" value="M" />
		<property name="train" ref="train1" />
	</bean>

	<bean class="com.jspiders.springcorexml.beans.PassengerBean"
		name="passenger2">
		<property name="id" value="2" />
		<property name="name" value="Priya" />
		<property name="age" value="80" />
		<property name="gender" value="F" />
		<property name="train" ref="train1" />
	</bean>

	<bean class="com.jspiders.springcorexml.beans.TrainBean"
		name="train1">
		<property name="id" value="1"/>
		<property name="train_no" value="123456"/>
		<property name="name" value="Deccan Express"/>
	</bean>

</beans>
